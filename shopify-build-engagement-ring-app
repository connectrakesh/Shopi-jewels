      <script type="text/javascript">
        
         var displaystep = getParameterByName('step');
//           $("#flag").val(displaystep);
//           var flag = $("#flag").val();

        function getParameterByName(name) {
            name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
            var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                results = regex.exec(location.search);
            return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
          }
        
        if(displaystep != "setting"){
 			 localStorage.setItem("variant_id", "");
             localStorage.setItem("diamond_id", "");
             localStorage.setItem("firstStep", "");
             localStorage.setItem("match_shape", "");
          
          //dimond filter reset if setting not match
            localStorage.setItem("dia_shape", "");
            localStorage.setItem("price_from", "100");
            localStorage.setItem("price_to", "500000");
            localStorage.setItem("carat_from", "0.2");
            localStorage.setItem("carat_to", "15");
            localStorage.setItem("cut_from", "0");
            localStorage.setItem("cut_to", "4");
            localStorage.setItem("color_from", "0");
            localStorage.setItem("color_to", "10");
            localStorage.setItem("clarity_from", "0");
            localStorage.setItem("clarity_to", "7");

            localStorage.setItem("symmetry_from", "0");
            localStorage.setItem("symmetry_to", "3");
            localStorage.setItem("polish_from", "0");
            localStorage.setItem("polish_to", "3");
            localStorage.setItem("table_from", "49");
            localStorage.setItem("table_to", "89");
            localStorage.setItem("depth_from", "45");
            localStorage.setItem("depth_to", "80");
            localStorage.setItem("fluorescence_from", "0");
            localStorage.setItem("fluorescence_to", "4");

            localStorage.setItem("report_from", "0");
        }
       //var firstStep = localStorage.getItem("firstStep");
      //alert(firstStep);
        var getDiamond = localStorage.getItem("diamond_id");  
         var variant_id = localStorage.getItem("variant_id");
        
        //alert(getDiamond);

        if (getDiamond && getDiamond.length > 0) {


          $("#diamond_first").show();
          $("#setting_first").hide();
          localStorage.setItem("firstStep", "Diamond");

         }
        else{

           $("#diamond_first").hide();
          $("#setting_first").show();
          
           localStorage.setItem("firstStep", "Setting");
        }

       


       //var div1 = document.getElementById("setting_sec");
       //var div2 = document.getElementById("setting_first");
       //var ring = document.getElementById("viewRing");


      if(getDiamond && variant_id){


        $(".viewRing").show();
        //edit_diamond.style.display = "block";
        $("#edit_diamond").show();


      }
      else if (getDiamond) {

        //edit_diamond.style.display = "block";
        $("#edit_diamond").show();


      }

      $(".view_dia").click(function(){

        var getDiamond = localStorage.getItem("diamond_id");

       window.location.href = "https://tps2015-2.myshopify.com/pages/diamond?diamond_id="+getDiamond+"&step=diamond";

      });

       $(".view_ring").click(function(){ 

          var variant_id = localStorage.getItem("variant_id");
          var getDiamond = localStorage.getItem("diamond_id");  

          window.location.href = "https://tps2015-2.myshopify.com/pages/viewring?diamond_id="+getDiamond+"&variant_id="+variant_id;

      });

