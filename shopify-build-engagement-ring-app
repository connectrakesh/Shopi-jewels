    <input type="hidden" name="diamond_filter_values" id="diamond_filter_values" value="">
  
  
  <script>
    
  //maintaine fillter session for style and shape  
    
   if(typeof(Storage) !== "undefined") { 
     
        var setting_shape_fillter  = localStorage.getItem("setting_shape_fillter")!= null?localStorage.getItem("setting_shape_fillter"):"";
   		var setting_style_fillter  = localStorage.getItem("setting_style_fillter")!= null?localStorage.getItem("setting_style_fillter"):"";
        
       //save value in hidden field 
        $("#style_fillter_hidden").val(setting_style_fillter);
		$("#diamond_filter_values").val(setting_shape_fillter);
     
     
       //set setting fillter if set in session 
         jQuery(".tps_style_fillter a").each(function(){
        	 if(setting_style_fillter.indexOf(jQuery(this).attr("title")) != -1){ 
     	    	jQuery(this).addClass("selected"); 
     		 }
         });
     
      //set setting shape fillter if set in session 
         jQuery("#tps_diamond_shapes a").each(function(){
        	 if(setting_shape_fillter.indexOf(jQuery(this).attr("title")) != -1){ 
     	    	jQuery(this).addClass("selected"); 
     		 }
         });
   
   }
     
    
    
  //     /setting style filter 
   jQuery("#tps_style_fillter a").click(function() {  

 	if(jQuery(this).hasClass('selected')){ 
     		jQuery(this).removeClass('selected');
     	}
     	else{
     		jQuery(this).addClass('selected');
     	}
        //get selected value store in array 
        var selected_settings=new Array(); 
        jQuery(".tps_style_fillter").find("a.selected").each(function(){
        	//selected_setting_array.push(jQuery(this).attr("title"));
        	 if(selected_settings==""){ 
     	    	  selected_settings=jQuery(this).attr("title"); 
     			}else{ 
     				selected_settings=selected_settings+","+jQuery(this).attr("title");
     			}
         });


        //jQuery("#style_fillter_hidden").val(JSON.stringify(selected_setting_array));
        jQuery("#style_fillter_hidden").val(selected_settings);
        localStorage.setItem("setting_style_fillter", selected_settings);
        
     //console.log(selected_settings);
        ajaxDataCall();
  });

    
    
    /* start Diamond Shapes */ 
       

    
     jQuery("#tps_diamond_shapes a").click(function() {  

 	if(jQuery(this).hasClass('selected'))
     	{ 
     		jQuery(this).removeClass('selected');
     		jQuery(this).find("div").removeClass('tps_inputrange_'+jQuery(this).attr("title")+'_select').addClass('tps_inputrange_'+jQuery(this).attr("title"));
     	}
     	else{
     		jQuery(this).addClass('selected');
     		jQuery(this).find("div").addClass('tps_inputrange_'+jQuery(this).attr("title")+'_select').removeClass('tps_inputrange_'+jQuery(this).attr("title"));
     	}
     	var diamond_selected='';
     	jQuery(".tps_inputrange_shape").find("a.selected").each(function(){
     	      if(diamond_selected==""){ 
     	    	  diamond_selected=jQuery(this).attr("title"); 
     			}else{ 
     				diamond_selected=diamond_selected+","+jQuery(this).attr("title");
     			}
     	       	   		   
         });
    
           //alert(diamond_selected);
     	  jQuery("#diamond_filter_values").val(diamond_selected);
          localStorage.setItem("setting_shape_fillter", diamond_selected);
      
    ajaxDataCall();
    
	
  });
  
  /* End Diamond Shapes */ 
  
      /* price slider */
    
      if(typeof(Storage) !== "undefined") { 
       	var setting_price_from 	= localStorage.getItem("setting_price_from")!= null?localStorage.getItem("setting_price_from"):100;
   		var setting_price_to 	= localStorage.getItem("setting_price_to")!= null?localStorage.getItem("setting_price_to"):10000;
      }
      else { 
       var setting_price_from 	= 100;
       var setting_price_to 	= 10000;
      }
        
       var symbol="£";    
    
        $('#calcAmount').val(symbol+addThousandSep(setting_price_from)); 
        $('#calcAmount2').val(symbol+addThousandSep(setting_price_to));
        $("#pricerange_min").val(setting_price_from);
		$("#pricerange_max").val(setting_price_to);
        
       
       var valueSlider 	= 	1;
       var min_price	=	jQuery( "#pricerange_min" ).val(); 
       var max_price	=	jQuery( "#pricerange_max" ).val();
   

         var value1 	= 	min_price.replace(',','.');  
         var value2 	= 	max_price.replace(',','.');
         var valueSlider 	= 	1;  
         var result 	= 	valueSlider * value1 * value2;
    
    price_slider_render(min_price,max_price,true);
	
    function price_slider_render(start,end,option)
	{

    //new filter for 
      
    jQuery('#amountSlider').slider({
     orientation: "horizontal",
     range: true, 
     values: [start, end],
        min: 100,
        max: 10000, 
        step: 100,
       slide: function (event, ui) {
         if ((ui.values[0]) >= (ui.values[1])) {
            return false;
               }
         var symbol="£";     
         jQuery("#pricerange_min").val(ui.values[0]); 
         jQuery("#pricerange_max").val(ui.values[1]); 
         jQuery('#calcAmount').val(symbol+addThousandSep(ui.values[0])); 
         jQuery('#calcAmount2').val(symbol+addThousandSep(ui.values[1]));
      },change: function( event, ui ) { 
         var symbol="£";     
         jQuery("#pricerange_min").val(ui.values[0]); 
         jQuery("#pricerange_max").val(ui.values[1]); 
         jQuery('#calcAmount').val(symbol+addThousandSep(ui.values[0])); 
         jQuery('#calcAmount2').val(symbol+addThousandSep(ui.values[1]));
       jQuery('#page_no').val('');
        
       localStorage.setItem("setting_price_from", ui.values[0]);
       localStorage.setItem("setting_price_to", ui.values[1]); 
       ajaxDataCall();
    }
   }); 
      
      
      
      

	}

	var multiplication = function () { 
		  var amountValue = jQuery('#amountSlider').slider('values',0);  
		  var amountValue2 = jQuery('#amountSlider').slider('values',1);
			var result = amountValue * value1 * value2;
		  var result2 = amountValue2 * value1 * value2;
			var steppingInCalc = jQuery('#amountSlider').slider('option', 'step');
			jQuery('#calcStepping').html(steppingInCalc); 
			jQuery('#calcAmount').val('£'+amountValue);
			jQuery('#calcAmount2').val('£'+amountValue2);
			jQuery('#pricerange_min').val(amountValue);
			jQuery('#pricerange_max').val(amountValue2); 
	};

	function change_price_to(){
		if(jQuery('#calcAmount2').val()== ''){
			var value = jQuery('#pricerange_max').val(); 
		}	else{
			 var value=jQuery('#calcAmount2').val();
		}
		var value1=jQuery('#calcAmount').val();
      
     	var value = value.replace('£','');
      	var value1 = value1.replace('£','');
      
         value = value.replace(',','');
      	 value1 = value1.replace(',','');
      
		value=parseFloat(value);
		value1=parseFloat(value1);
		if(value1> value){ 
			alert('Price from should be less than price to');
			price_slider_render(value1,max_price,true);
			return false;
		}
		jQuery('#pricerange_max').val(value);
		jQuery('#pricerange_min').val(value1);
		price_slider_render(value1,value,true);
	
	}
	function change_price_from(){
		if(jQuery('#calcAmount').val()==''){
			var value = jQuery('#pricerange_min').val(); 
		}	else{
			 var value=jQuery('#calcAmount').val();
		}
		

		var value1=jQuery('#calcAmount2').val();
      
      	var value = value.replace('£','');
      	var value1 = value1.replace('£','');
        
        value = value.replace(',','');
      	value1 = value1.replace(',','');
        
		
		value=parseFloat(value);
		value1=parseFloat(value1);
		if(value > value1){
			alert("Price from should be less than price to");
			price_slider_render(min_price,value1,true);
			return false;
		}
		jQuery('#pricerange_min').val(value);
		jQuery('#pricerange_max').val(value1);
		price_slider_render(value,value1,true);
	
	}
         /* price slider */
        

    ajaxDataCall();
     function ajaxDataCall(){
          var json = doProducts("shopify-build-engagement-ring-app",500);
            $.ajax({
	        type: 'GET',
	        url:json,
	        dataType: 'jsonp',
	        success: function (data) {
	        var output = doFilter(data);
                
           var product_count = output.length;
                
              $('#sandBox').empty();
      
              if(parseInt(product_count) > 0){
                
               $.each(output, function(i, product) {
               
                        if (product.images.length > 0) {
                            var tempSrc = product.images[0].src;
                        } else {
                            var tempSrc = imgsrc;
                        }
                        $('#sandBox').append('<li class="element first no_full_width" data-price="50000" data-alpha="'+product.title+'"><ul class="row-container list-unstyled clearfix"><li class="row-left"><a class="container_item" href="/collections/settings/products/'+product.handle+'"><img alt="'+product.title+'" class="img-responsive" src="'+tempSrc+'"></a></li><li class="row-right parent-fly animMix"><div class="product-content-left"><a href="/collections/settings/products/'+product.handle+'" class="title-5">'+product.title+'</a></div><div class="product-content-right"><div class="product-price"><span class="price">'+product.variants[0].price+'</span></div></div></li></ul>');
                
                });

             
              
              }else{
               
                 $('#sandBox').append('<div class="no_product">No Products found for selected criteria</div>');
              
              }    
              
			
                tpscustompagination(product_count,8);
            } 

	      });
        }
    
    var doFilter = function(data) {	
      var min 	= jQuery("#pricerange_min").val(); // price_from
      var max 	= jQuery("#pricerange_max").val(); // price_to 
        
        min = typeof min !== 'undefined' ? min : 100;
		max = typeof max !== 'undefined' ? max : 10000;
      
        //for style filter
        var style_fillter_values=jQuery("#style_fillter_hidden").val();
        var diamond_filter_values=jQuery("#diamond_filter_values").val();
		var output = new Array();
	    for (var i = 0; i < data.products.length; i++) {
            
           var flagsettingshape=1;   //set true if not apply fillter
           var flagsettingin=1;      //set true if not apply fillter
           
          
           var flagprice=0; // defualt set false
           
          //for setting style fillter 
         
          if(style_fillter_values!=""){
               flagsettingin=0;
              if(!(typeof data.products[i].options[2] === "undefined")){

                 jQuery(data.products[i].options[2].values).each(function(index, value){
                  //console.log(data.products[i].options[1].values);
                  // console.log(style_fillter_values.indexOf(value));
                    if(style_fillter_values.indexOf(value) !== -1){
                        flagsettingin=1;
                    }

                  });
              }
            }
          
           //end setting stylefiltter 
          
           //********** start configrabale shape fillter ***************/ 
          if(diamond_filter_values!=""){
           flagsettingshape=0;
           if(!(typeof data.products[i].options[1] === "undefined")){
             jQuery(data.products[i].options[1].values).each(function(index, value){
              //console.log(data.products[i].options[1].values);
               //console.log(diamond_filter_values.indexOf(value));
                if(diamond_filter_values.indexOf(value) !== -1){
                  flagsettingshape=1;
                 }
           		});
         	 }
            
          }
           //**************** end shape fillter  ****************************
            
          /****************  for price filter  ****************************/
	         if (data.products[i].variants[0].price >=parseFloat(min) && data.products[i].variants[0].price <= parseFloat(max)) {
	        	
               flagprice=1;
	         }
         
          /********************** //for price fillter  **************/
         
         //if product match all criteria then add in to product array
         if((flagsettingshape==1) && (flagsettingin==1) && (flagprice==1)){
         	output.push(data.products[i]);
         }else{
           output.data;
         }

	    }
	    return output;
	}
	
    function doProducts(collection, limit) {
		collection = typeof collection !== 'undefined' ? collection : 'settings';
		limit = typeof limit !== 'undefined' ? limit : 100;
		var url = 'https://tps2015-2.myshopify.com/collections/' + collection + '/products.json?limit=' + limit;
		return url;
	}	
    

  function addThousandSep(nStr) {
  	var tseparator=",";
    return nStr.toString().replace(/\B(?=(\d{3})+(?!\d))/g,tseparator);
 }
    
    
      </script>
 


