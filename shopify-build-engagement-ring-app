<script type="text/javascript">
$(document).ready(function() {

      imagesLoaded( '#sandBox', function() {
        var $container = $('#sandBox');
        
        $container.isotope({
          itemSelector : '.element',
          layoutMode : 'fitRows',
          getSortData : {
            name : function( $elem ) {
              return $elem.attr('data-alpha');
            },
            price : function( $elem ) {
              return parseInt( $elem.attr('data-price'), 10 );
            }
          }
        });
        
        
        var $optionSets = $('#options .option-set'),
            $optionLinks = $optionSets.find('li');
        
        $optionLinks.click(function(){
          var $this = $(this);
          // don't proceed if already selected
          if ( $this.hasClass('selected') ) {
            return false;
          }
          
          var $direction = $(this).attr('data-order');
          var key1 = 'sortAscending',
              value1 = true;
          switch($direction){
            case 'asc':
              value1 = true;
              break;
              
            case 'desc':
              value1 = false;
              break;
          }
          
          var $optionSet = $this.parents('.option-set');
          $optionSet.find('.selected').removeClass('selected');
          $this.addClass('selected');
          
          // make option object dynamically, i.e. { filter: '.my-filter-class' }
          var options = {},
              key = $optionSet.attr('data-option-key'),
              value = $this.attr('data-option-value');
          // parse 'false' as false boolean
          value = value === 'false' ? false : value;
          options[ key ] = value;
          options[ key1 ] = value1;
          if ( key === 'layoutMode' && typeof changeLayoutMode === 'function' ) {
            // changes in layout modes need extra logic
            changeLayoutMode( $this, options )
          } else {
            // otherwise, apply new options
            $container.isotope( options );
          }
          
          return false;
        });
      });
  });

</script>

<script>
  
    var tp=22
       // alert(tp);
  // tpscustompagination(22,8);
    
    function tpscustompagination(number_of_items,show_per_page){
  
                      var number_of_pages = Math.ceil(number_of_items/show_per_page);
  
      if(number_of_pages == 1){
        $('.pagination').hide();
      }else{
        $('.pagination').show();
      }
                      //set the value of our hidden input fields
                      $('#current_page').val(0);
                      $('#show_per_page').val(show_per_page);
                      $('#number_of_pages').val(number_of_pages);
                      
                      var navigation_html ="";

                	  navigation_html  +='<ul class="list-inline">';
                      navigation_html  += '<li class="prev previous_link" style = "display:none"><a href="javascript:previous();" ><i class="fa fa-angle-double-left"></i> Prev</a></li>';
                  
                      var current_link = 0;
               
                      while(number_of_pages > current_link){
                        navigation_html += '<li class="page_link" longdesc="' + current_link +'"><a href="javascript:go_to_page(' + current_link +')" >'+ (current_link + 1) +'</a></li>';
                        current_link++;
                      }
                     
               
                      if($('#current_page').val() < number_of_pages){
                         navigation_html += '<li class="next next_link"><a href="javascript:next();">Next <i class="fa fa-angle-double-right"></i></a></li>';
                      }
                      navigation_html  +='</ul>';
                      
      				 if(number_of_items > 0){
      				  navigation_html  +='<p class="pagination-num-showing hidden-xs">   Showing 1 - '+show_per_page+' of '+number_of_items+' total  </p>';
                     }
                       
                       //alert(navigation_html);
                      $('.pagination').html(navigation_html);

                      //add active class to the first page link
                      $('.pagination .page_link:first').addClass('active');

                      //hide all the elements inside content div
                      $('#sandBox').children().css('display', 'none');

                      //and show the first n (show_per_page) elements
                      $('#sandBox').children().slice(0, show_per_page).css('display', 'block');

               } 
  //  }
     
  
  
  
 function previous(){
          //  alert($('#current_page').val());

              new_page = parseInt($('#current_page').val()) - 1;
              //if there is an item before the current active link run the function
              if($('.active').prev('.page_link').length==true){
                go_to_page(new_page);
              }

            }

            function next(){
              new_page = parseInt($('#current_page').val()) + 1;
              //if there is an item after the current active link run the function
              if($('.active').next('.page_link').length==true){
                go_to_page(new_page);
              }

            }
            function go_to_page(page_num){
             var number_of_pages = $('#number_of_pages').val();
              //get the number of items shown per page
              var show_per_page = parseInt($('#show_per_page').val());

              //get the element number where to start the slice from
              start_from = page_num * show_per_page;

              //get the element number where to end the slice
              end_on = start_from + show_per_page;
              
              //get the total count
              var totalproductcount='22';
              totalproductcount=parseInt(totalproductcount);
              
              var startproductnumber="";
              var endproductnumber="";
              
         
              if((parseInt(totalproductcount) < parseInt(show_per_page))|| ((parseInt(number_of_pages)-1) == parseInt(page_num))){
               endproductnumber=totalproductcount;
              }else {
               endproductnumber=((page_num +1) * show_per_page);
              }
              
              startproductnumber=(page_num * show_per_page) + 1;
              
              if(parseInt(totalproductcount) > 0){
              	$('.pagination-num-showing').html('Showing '+startproductnumber+' - '+endproductnumber+' of '+totalproductcount+' total');
              }else{
                $('.pagination-num-showing').html('');
              }
              
              //hide all children elements of content div, get specific items and show them
              $('#sandBox').children().css('display', 'none').slice(start_from, end_on).css('display', 'block');

              /*get the page link that has longdesc attribute of the current page and add active class to it
              and remove that class from previously active page link*/
              $('.page_link[longdesc=' + page_num +']').addClass('active').siblings('.active').removeClass('active');

              if(page_num > 0)
              {
                $('.previous_link').show();
              }else{
                $('.previous_link').hide();
              }
              if(number_of_pages == page_num+1)
              {
                $('.next_link').hide();
              }else{
                $('.next_link').show();
              }

              //update the current page input field
              $('#current_page').val(page_num);
            }    
  
  // reset filter
  function resetfilter(){    
    localStorage.setItem("setting_shape_fillter", "");  
    localStorage.setItem("setting_style_fillter", ""); 
    localStorage.setItem("setting_price_from", "100");
    localStorage.setItem("setting_price_to", "10000");
       
    
    window.location.reload();
  }
</script>
              
              
              
            </div>
          </div>
          
          

        </section>        
      </div>
    </div>
  </div>

